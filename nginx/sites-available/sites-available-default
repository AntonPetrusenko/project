server {
    listen 80 default_server;
    listen [::]:80 default_server;

    server_name noname;

    root /var/www/html/empty-project;

    index index.php index.html index.htm;

    location / {
        try_files $uri $uri/ /index.php?$args;
    }

    location ~ \.php$ {
        fastcgi_pass php:9000;
    }
}
